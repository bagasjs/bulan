function main() 
{
    // Change the value of a to get different result
    var a, c;
    a = 1;
    c = 0;

    if(a > 10) {
        c = -1;
    } else {
        c = 1;
    }

    while(a != 10) {
        fprintf(stdout, "a is %d", c);
        a = a + c;
    }

    return 0;
}
